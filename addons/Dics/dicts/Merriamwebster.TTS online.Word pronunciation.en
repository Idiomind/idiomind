#!/bin/bash

if [[ "$1" = _dlg_ ]]; then
    source "$DS/ifs/cmns.sh"
    msg "$(gettext "Languages"): English\n$(gettext "Does not need configuration")\n" dialog-information "$4" "$(gettext "Close")"
else
    if [ ${#word} -lt 6 ]; then
        c=0000000
        export LINK="http://media.merriam-webster.com/soundc11/${word:0:1}/${word}${c::-${#word}}1.wav"
        export ex='wav'
    fi
fi
