#!/bin/bash

if [[ "$1" = dlgcnfg ]]; then
source "$DS/ifs/mods/cmns.sh"
msg "$(gettext "Does not need configuration")\n" dialog-information "$4"

else
if [ ${#word} -lt 6 ]; then
    c=0000000
    export LINK="http://media.merriam-webster.com/soundc11/${word:0:1}/${word}${c::-${#word}}1.wav"
    export ex='wav'
fi
fi
